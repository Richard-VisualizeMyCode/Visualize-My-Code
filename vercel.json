{
    "builds": [
        {
            "src": "backend/generate_backend.py",
            "use": "@vercel/python",
            "config": {
                "includeFiles": "**/*.py",
                "excludeFiles": "**/__pycache__/**,**/*.pyc,**/*.log,**/*.md,**/tests/**,**/docs/**"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/backend/generate_backend.py"
        }
    ]
}